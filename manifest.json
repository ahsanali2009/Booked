{
    "manifest_version": 3,
    "name": "Bookmarks Manager",
    "description": "A Chrome extension to manage bookmarks.",
    "version": "1.0",
    "permissions": [
      "bookmarks"
    ],
    "background": {
      "service_worker": "script.js"
    },
    "action": {
      "default_popup": "index.html",
      "default_icon": "/images/faviconimg.png"
    }
  }
  